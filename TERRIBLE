local LocalPlayer = Players.LocalPlayer

-- Safely destroy if it exists
local oldGui = CoreGui:FindFirstChild("FantisyScriptUITuff")
if oldGui then
    oldGui:Destroy()
end

-- Create new ScreenGui
local newGui = Instance.new("ScreenGui")
newGui.Name = "SaggintUI"
newGui.ResetOnSpawn = false
newGui.Parent = CoreGui

-- Create Frame
local frame = Instance.new("Frame")
frame.Size = UDim2.new(0, 340, 0, 120)
frame.Position = UDim2.new(0.5, -170, 0.5, -60)
frame.BackgroundColor3 = Color3.fromRGB(10, 10, 10)
frame.BorderSizePixel = 0
frame.Parent = newGui
frame.Active = true
frame.Draggable = true

-- Add UICorner
local corner = Instance.new("UICorner")
corner.CornerRadius = UDim.new(0, 16)
corner.Parent = frame

-- Add UIStroke
local stroke = Instance.new("UIStroke")
stroke.Color = Color3.fromRGB(255, 140, 255)
stroke.Thickness = 2
stroke.Transparency = 0.2
stroke.Parent = frame

-- Add TextLabel
local label = Instance.new("TextLabel")
label.Size = UDim2.new(1, 0, 1, 0)
